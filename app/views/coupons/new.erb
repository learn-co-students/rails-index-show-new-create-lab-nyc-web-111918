<h2>Coupon Form</h2>

<%= form_tag coupons_path do %>
  <label for="store">Store: </label><%= text_field_tag :"coupon[store]" %>
  <label for="code">Code: </label><%= text_field_tag :"coupon[coupon_code]" %>

  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>

  <%= submit_tag value = "Submit Coupon" %>
<% end %><br>

<a href="/coupons">Home</a>
